{
  "name": "@wardbrian/tree-sitter-stan",
  "version": "0.2.4",
  "description": "A tree sitter grammer for modern Stan programs",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter generate && tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "author": "Brian Ward",
  "license": "MIT",
  "dependencies": {
    "node-gyp": "^12.1.0",
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.23.2",
    "prebuildify": "^6.0.1"
  },
  "tree-sitter": [
    {
      "scope": "source.stan",
      "file-types": [
        "stan"
      ],
      "highlights": "queries/highlights.scm",
      "tags": "queries/tags.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "gypfile": true,
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WardBrian/tree-sitter-stan.git"
  },
  "keywords": [
    "tree-sitter",
    "stan",
    "parsing"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "bugs": {
    "url": "https://github.com/WardBrian/tree-sitter-stan/issues"
  },
  "homepage": "https://github.com/WardBrian/tree-sitter-stan#readme"
}
