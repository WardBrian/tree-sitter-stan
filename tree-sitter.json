{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "stan",
      "camelcase": "Stan",
      "title": "Stan",
      "scope": "source.stan",
      "file-types": [
        "stan"
      ],
      "injection-regex": "^stan$",
      "class-name": "TreeSitterStan"
    }
  ],
  "metadata": {
    "version": "0.2.5",
    "license": "MIT",
    "description": "A parser for the Stan modelling language",
    "authors": [
      {
        "name": "Brian Ward",
        "email": "bward@flatironinstitute.org",
        "url": "https://brianward.dev"
      }
    ],
    "links": {
      "repository": "https://github.com/WardBrian/tree-sitter-stan"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
